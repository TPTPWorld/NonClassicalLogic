tff('k_branch_p.0001',logic,
    $modal == 
      [ $modalities == $modal_system_K ] ).

tff(p0_decl,type,
    p0: $o ).

tff(p1_decl,type,
    p1: $o ).

tff(p100_decl,type,
    p100: $o ).

tff(p101_decl,type,
    p101: $o ).

tff(p102_decl,type,
    p102: $o ).

tff(prove,conjecture,
    ~ ~ ( ~ ( p100
            & ~ p101
            & ( p101
             => p100 )
            & ( p102
             => p101 )
            & ( p100
             => ( ( p0
                 => [.] ( p100
                       => p0 ) )
                & ( ~ p0
                 => [.] ( p100
                       => ~ p0 ) ) ) )
            & ( p101
             => ( ( p1
                 => [.] ( p101
                       => p1 ) )
                & ( ~ p1
                 => [.] ( p101
                       => ~ p1 ) ) ) )
            & ( ( p100
                & ~ p101 )
             => ( <.> ( p101
                      & ~ p102
                      & p1 )
                & <.> ( p101
                      & ~ p102
                      & ~ p1 ) ) )
            & [.] ( ( p101
                   => p100 )
                  & ( p102
                   => p101 )
                  & ( p100
                   => ( ( p0
                       => [.] ( p100
                             => p0 ) )
                      & ( ~ p0
                       => [.] ( p100
                             => ~ p0 ) ) ) )
                  & ( p101
                   => ( ( p1
                       => [.] ( p101
                             => p1 ) )
                      & ( ~ p1
                       => [.] ( p101
                             => ~ p1 ) ) ) )
                  & ( ( p100
                      & ~ p101 )
                   => ( <.> ( p101
                            & ~ p102
                            & p1 )
                      & <.> ( p101
                            & ~ p102
                            & ~ p1 ) ) ) ) )
        | ~ [.] p1 ) ).

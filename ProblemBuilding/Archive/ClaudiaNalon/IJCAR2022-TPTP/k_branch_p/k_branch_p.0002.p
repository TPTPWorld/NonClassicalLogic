                      & ~ p102
                      & ~ p1 ) ) )
            & ( ( p101
                & ~ p102 )
             => ( <.> ( p102
                      & ~ p103
                      & p2 )
                & <.> ( p102
                      & ~ p103
                      & ~ p2 ) ) )
            & [.] ( ( p101
                   => p100 )
                  & ( p102
                   => p101 )
                  & ( p103
                   => p102 )
                  & ( p100
                   => ( ( p0
                       => [.] ( p100
                             => p0 ) )
                      & ( ~ p0
                       => [.] ( p100
                             => ~ p0 ) ) ) )
                  & ( p101
                   => ( ( p1
                       => [.] ( p101
                             => p1 ) )
                      & ( ~ p1
                       => [.] ( p101
                             => ~ p1 ) ) ) )
                  & ( p102
                   => ( ( p2
                       => [.] ( p102
                             => p2 ) )
                      & ( ~ p2
                       => [.] ( p102
                             => ~ p2 ) ) ) )
                  & ( ( p100
                      & ~ p101 )
                   => ( <.> ( p101
                            & ~ p102
                            & p1 )
                      & <.> ( p101
                            & ~ p102
                            & ~ p1 ) ) )
                  & ( ( p101
                      & ~ p102 )
                   => ( <.> ( p102
                            & ~ p103
                            & p2 )
                      & <.> ( p102
                            & ~ p103
                            & ~ p2 ) ) ) )
            & [.] [.] ( ( p101
                       => p100 )
                      & ( p102
                       => p101 )
                      & ( p103
                       => p102 )
                      & ( p100
                       => ( ( p0

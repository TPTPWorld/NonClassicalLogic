                                 => p0 ) )
                          & ( ~ p0
                           => [.] ( p100
                                 => ~ p0 ) ) ) )
                      & ( p101
                       => ( ( p1
                           => [.] ( p101
                                 => p1 ) )
                          & ( ~ p1
                           => [.] ( p101
                                 => ~ p1 ) ) ) )
                      & ( p102
                       => ( ( p2
                           => [.] ( p102
                                 => p2 ) )
                          & ( ~ p2
                           => [.] ( p102
                                 => ~ p2 ) ) ) )
                      & ( ( p100
                          & ~ p101 )
                       => ( <.> ( p101
